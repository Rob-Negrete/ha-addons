{
  "name": "Face Rekon",
  "version": "2.0.1",
  "slug": "face_rekon",
  "description": "AI-powered face recognition with comprehensive web UI for managing and labeling faces. Features real-time detection, FAISS similarity search, and seamless Home Assistant integration.",
  "startup": "application",
  "arch": [
    "amd64",
    "armv7",
    "aarch64"
  ],
  "boot": "auto",
  "map": [
    "config:rw",
    "addons:rw"
  ],
  "host_network": false,
  "ingress": true,
  "ingress_port": 5001,
  "panel_icon": "mdi:face-recognition",
  "panel_title": "Face Recognition Management",
  "webui": "http://[HOST]:[PORT:5001]/",
  "options": {
    "db_path": "/config/face-rekon/db",
    "faces_path": "/config/face-rekon/faces",
    "unknowns_path": "/config/face-rekon/unknowns",
    "index_path": "/config/face-rekon/db/faiss_index.index"
  },
  "schema": {
    "db_path": "str",
    "faces_path": "str",
    "unknowns_path": "str",
    "index_path": "str"
  },
  "ports": {
    "5001/tcp": 5001
  },
  "ports_description": {
    "5001/tcp": "Web UI and API endpoints"
  },
  "log_url": "/config/face-rekon/logs"
}
