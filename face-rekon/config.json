{
  "name": "Face Rekon",
  "version": "1.0.11",
  "slug": "face_rekon",
  "description": "Reconocimiento facial con almacenamiento en TinyDB y comparaci√≥n con FAISS",
  "startup": "application",
  "arch": ["amd64", "armv7", "aarch64"],
  "boot": "auto",
  "map": ["config:rw", "addons:rw"],
  "host_network": false,
  "options": {
    "db_path": "/config/face-rekon/db",
    "faces_path": "/config/face-rekon/faces",
    "unknowns_path": "/config/face-rekon/unknowns",
    "index_path": "/config/face-rekon/db/faiss_index.index"
  },
  "schema": {
    "db_path": "str",
    "faces_path": "str",
    "unknowns_path": "str",
    "index_path": "str"
  },
  "ports": {
    "5001/tcp": 5001
  },
  "log_url": "/config/face-rekon/logs"
}